<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.salimsp.testyouriq">

    <application
        android:allowBackup="true"
        android:icon="@drawable/iqlogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name=".SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuizAct"
            android:screenOrientation="portrait"/>
        <activity android:name=".ScoreBoard"
            android:screenOrientation="portrait"/>
        <activity android:name=".Question"
            android:screenOrientation="portrait"/>
        <activity android:name=".Answer" android:screenOrientation="portrait"/>
        <activity android:name=".Start"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>